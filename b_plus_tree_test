	rankdir=TB
	node0 [label="Internal Node\n6" fillcolor=lightgreen shape=record style=filled]
	node1 [label="Internal Node\n2 | 4" fillcolor=lightgreen shape=record style=filled]
	node2 [label="Leaf Node\n0 | 1" fillcolor=lightblue shape=record style=filled]
