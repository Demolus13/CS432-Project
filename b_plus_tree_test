	node3 [label="Leaf Node\n2 | 3" fillcolor=lightblue shape=record style=filled]
	node1 -> node3 [label="≤ 4"]
	node4 [label="Leaf Node\n4" fillcolor=lightblue shape=record style=filled]
	node1 -> node4 [label="> 4"]
	node0 -> node1 [label="≤ 6"]
	node5 [label="Internal Node\n8" fillcolor=lightgreen shape=record style=filled]
	node6 [label="Leaf Node\n6 | 7" fillcolor=lightblue shape=record style=filled]
	node5 -> node6 [label="≤ 8"]
	node7 [label="Leaf Node\n8 | 9" fillcolor=lightblue shape=record style=filled]
	node5 -> node7 [label="> 8"]
	node0 -> node5 [label="> 6"]
	node7 -> node7 [color=red constraint=false style=dashed]
	node7 -> node7 [color=red constraint=false style=dashed]
	node7 -> node7 [color=red constraint=false style=dashed]
	node7 -> node7 [color=red constraint=false style=dashed]
